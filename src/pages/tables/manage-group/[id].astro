---
import Layout from "../../../layouts/Layout.astro";
import Maincontent from "../../../layouts/Maincontent.astro";
import ManageGroup from "../../../components/manage-group.tsx";
const authToken = Astro.cookies.get('auth_token');

if (!authToken) { return Astro.redirect('/login');}

const { id } = Astro.params;
---

<Maincontent title="Manage Group">
  <ManageGroup id={id} client:load/>
</Maincontent>

<Layout title="Manage Group" />
